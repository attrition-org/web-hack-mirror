function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

//preload images for button rollovers

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		SageWave_01_over = newImage("images/SageWave_01-over.gif");
		preloadFlag = true;
	}


	if (document.images) {
		ResourceGuide_01_over = newImage("images/ResourceGuide_01-over.gif");
		preloadFlag = true;
	}

	if (document.images) {
		ViewWebinar_01_over = newImage("images/ViewWebinar_01-over.gif");
		preloadFlag = true;
	}

	if (document.images) {
		ViewQA_01_over = newImage("images/ViewQA_01-over.gif");
		preloadFlag = true;
	}
}


