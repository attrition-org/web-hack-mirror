var now = new Date();
var dName = now.getDay() + 1;

if(dName==1) Day = "Domingo";
if(dName==2) Day = "Segunda";
if(dName==3) Day = "Ter&ccedil;a";
if(dName==4) Day = "Quarta";
if(dName==5) Day = "Quinta";
if(dName==6) Day = "Sexta";
if(dName==7) Day = "S&aacute;bado";

var todaysDia =(""+ Day +"");
</SCRIPT>

