<script language="JavaScript">
function munculmaj() {
   if (document.all) {
      lmaj1.style.left = window.event.x + document.body.scrollLeft;
	  lmaj1.style.top = window.event.y + document.body.scrollTop;	
      lmaj1.style.visibility ="visible";
      setTimeout("sembunyimaj()",2500); }
   else if (document.layers) {
      document.lmaj0.left = Xpos;
	  document.lmaj0.top = Ypos;
      document.lmaj0.visibility ="show";
   }
}
function sembunyimaj() {
   if (document.all)
      lmaj1.style.visibility ="hidden";
   else if (document.layers) document.lmaj0.visibility = "hide";
}
</script>

