<script language="JavaScript">
var Xpos;
var Ypos;
function MoveHandler(e)
{
    Xpos = e.pageX;
    Ypos = e.pageY;	  
    return true;
}
if (document.layers) {
   document.captureEvents(Event.MOUSEMOVE);
   document.onMouseMove = MoveHandler;
}
function muncul() {
   if (document.all) {
      lbuku1.style.left = window.event.x + document.body.scrollLeft;
	  lbuku1.style.top = window.event.y + document.body.scrollTop;
      lbuku1.style.visibility ="visible";
      setTimeout("sembunyi()",2500); }
   else if (document.layers) {
      
      document.lbuku0.left = Xpos;
	  document.lbuku0.top = Ypos;
      document.lbuku0.visibility ="show";
   }
}
function sembunyi() {
   if (document.all)
      lbuku1.style.visibility ="hidden";
   else if (document.layers) document.lbuku0.visibility = "hide";
}
</script>
