function civiewer1 (url)
{
var civiewerpop;
        civiewerpop = window.open (url,'civiewer','location=no,toolbar=no,directories=no,menubar=no,resizable=no,scrollbars=yes,status=no,width=600,height=400,left=0,top=0');
                if ( version == "ver1" )
        civiewerpop.focus();
}
</script>

