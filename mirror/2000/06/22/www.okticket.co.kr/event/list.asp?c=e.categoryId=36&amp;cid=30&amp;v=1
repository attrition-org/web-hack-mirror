						<SCRIPT language=javascript>

	var actions = "/event/list.asp";
	
	function runIndex(conditions)
	{
		if(actions == "/event/list.asp")
		{
			window.open(actions + "?c=e." + conditions, "_self");
		}
		else
		{
			window.open(actions + "?c=" + conditions, "_self");
		}
	}
	
</SCRIPT>

