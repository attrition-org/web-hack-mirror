                  <form 
                    method=get 
                    action="/search" 
                    name="form1"
                    onsubmit="
                       if ( this.q.value == '' )
                       {
                           return false;
                       }
                       else
                       {
                        var url = '/search?q=' + escape( this.q.value );
                        for ( var i = 0; i < this.section.length; i++ )
                        {
                            if( this.section[i].checked )
                            {
                                url = url + '&section=' + this.section[i].value;
                            }
                        }
                        window.open( url, '_self' );
                       }
                    "
                >
