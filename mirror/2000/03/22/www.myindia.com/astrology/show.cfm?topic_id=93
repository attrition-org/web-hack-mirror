function chkLog()
{
if (document.mform.user.value=="")
{
alert("Login ID cannot be blank");
document.mform.user.focus();
return false;
}
if (document.mform.password.value=="")
{
alert("Password cannot be blank");
document.mform.password.focus();
return false;
}
return true;
}
</script>
