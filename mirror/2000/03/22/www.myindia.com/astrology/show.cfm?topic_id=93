<td valign=top><script language="JavaScript">
function chkdate()
{
 cur_month=document.astro.birth_month.value;
 cur_day=document.astro.birth_day.value;
 if (cur_month==2 && cur_day>29)  
  { 
  alert('Days Cannot not be more than 29');
  document.astro.birth_day.focus();
  }
  else
  {
   if ((cur_month==4 || cur_month==6 || cur_month==9 || cur_month==11) && cur_day>30)
    { 
      alert('Days Cannot not be more than 30');
      document.astro.birth_day.focus();
    }
	else
	{
	mon=document.astro.birth_month.selectedIndex+1;
	day=document.astro.birth_day.selectedIndex+1;
window.open('/astrology/daily.cfm?birth_month='+mon+'&birth_day='+day,'','width=400,height=250','scrollbars=yes');
	}
  }
}
</script>
