<script>
function chk()
{
	if (document.my.user.value=="")
	{
		alert("Sorry, but the big giant head found your login id unacceptable. Either you have used special characters or not used any characters at all");
		document.my.user.focus();
		return false;
	}
	else
	{	
		flag=0;
		stri = new String(document.my.user.value);
		for(i=0;i<stri.length;i++)
		{	
			us=stri.charCodeAt(i);
			if (us<48 || (us>57 && us<65) || (us>90 && us<97) || us>122)
			{
				if (us!=45 && us!=46 && us!=95)
					flag++;
			}
			if (flag > 0) break;
		}
		if (flag>0)
		{
			alert("Special Characters not allowed in Login ID.");
			document.my.user.focus();
			return false;
		}
		
		if (stri.length<4) 
		{
			alert('Login ID cannot be less than 4 characters');
			document.my.user.focus();
			return false;
		}
   	}
	
	if (document.my.password.value=="")
	{
		alert("You must be in a real hurry 'cause you forgot to enter your password.");
		document.my.password.focus();
		return false;
	}
	else
	{	
		flag=0;
		stri = new String(document.my.password.value);
		for(i=0;i<stri.length;i++)
		{	
			us=stri.charCodeAt(i);
			if (us<48 || (us>57 && us<65) || (us>90 && us<97) || us>122)
			{
				if (us!=45 && us!=46 && us!=95)
					flag++;
			}
			if (flag > 0) break;
		}
		if (flag>0)
		{
			alert("Special Characters not allowed in Password.");
			document.my.password.focus();
			return false;
		}
		
		if (stri.length<4) 
		{
			alert('Password cannot be less than 4 characters');
			document.my.password.focus();
			return false;
		}
   	 }
	
	if (document.my.first_name.value=="")
	{
		alert("It's ok, you not the only one with an embarrassing name. Don't feel shy; enter your First Name");
		document.my.first_name.focus();
		return false;
	}
	
	if (document.my.last_name.value=="")
	{
		alert("Your family may have disowned you but you can still use the family name. So fill up our Last Name");
		document.my.last_name.focus();
		return false;
	}
	
	if (document.my.cnt_id.value==0)
	{
		alert("Select a Country");
		document.my.cnt_id.focus();
		return false;
	}
		
	if (document.my.address.value=="")
	{
		alert("If you don't give us your address we won't be able to send you your prizes.");
		document.my.address.focus();
		return false;
	}

// if zip code is not null checking whether it is numeric or not

/*	if (document.my.zip.value!="")
	{
		if (isNaN(document.my.zip.value))
		{
		alert("Do you really expect us to believe that this is your Zip Code");
		document.my.zip.value="";
		document.my.zip.focus();
		return false;
		}
	} */
	
	
// if phone no is not null checking whethet it is numeric and if it is less than 7 numbers

 	if (document.my.Phone_no.value!="")
	{
		stri = new String(document.my.Phone_no.value);
	 	if ((stri.length < 7) || (isNaN(document.my.Phone_no.value)))
		{
			alert("Wrong telephone number, Please fill in the correct one.");
			document.my.Phone_no.focus();
			return false;
		}
	}

// if year of birth is entered then checking if it is numeric or not

	if (document.my.ydob.value!="")
	{
		if (isNaN(document.my.ydob.value))
		{
		alert("May be you should check your birth certificate again 'cause there is someting wrong with your Year of Birth.");
		document.my.ydob.value="";
		document.my.ydob.focus();
		return false;
		}

		if (document.my.ddob.value==0)
		{
		alert("Select a Date of Birth");
		document.my.ddob.focus();
		return false;
		}

		if (document.my.mdob.value==0)
		{
		alert("Select a Month of Birth");
		document.my.mdob.focus();
		return false;
		}
	}

// if email is entered and if it is valid 

   if (document.my.alt_email.value!="")
   {
	flag1=0;
	flag2=0;
	stri = new String(document.my.alt_email.value);
	for(i=0;i<stri.length;i++)
	{
		//alert(stri.substr(i,1));
		if (stri.substr(i,1) =="@")
		{
			flag1++;
		}
		if (stri.substr(i,1) ==".")
		{
			flag2++;
		}
		if (flag1 > 0 && flag2 > 0) break;
	}

	if (flag1==0 || flag2 == 0)
	{
		alert("If you don't have an alternate Email ID, just say so. There is no need to fake one.");
		document.my.alt_email.focus();
		document.my.alt_email.select();
		return false;
	}


	at=stri.indexOf("@");
