//function to generate a random number between 1 and max.

function rndnumber(max){
        var randscript = -1
        while (randscript < 1 || randscript > max || isNaN(randscript)){
                randscript = parseInt(Math.random()*(max+1))
        }
        return randscript
}

//function to generate the random Blues Name.

function bluesname() {
   subscript=rndnumber(w1max)
   msg = w1[subscript]
   subscript=rndnumber(w2max)
   msg = msg + " " + w2[subscript]
   subscript=rndnumber(w3max)
   msg = msg + " " + w3[subscript]
   return msg
}

