<SCRIPT language="JavaScript">

var browser="WRONG";
if (navigator.appName.substring(0,8)=="Netscape" &&
    Math.round(parseFloat(navigator.appVersion)) >= 3.0) browser="NET";
else if (Math.round(parseFloat(navigator.appVersion)) >= 4.0 ) browser="EXP";


if (browser!="WRONG") 
{
 bol=new Image();
 pijl=new Image();
 bol_on=new Image();
 pijl_on=new Image();
 bol.src="buttons/buttonbol.gif";
 bol_on.src="buttons/buttonbol_on.gif";
 pijl.src="buttons/buttonpijl.gif";
 pijl_on.src="buttons/buttonpijl_on.gif";
}

function hiStart(ibol,ipijl) 
 {
  if (browser!="WRONG") 
  {
   document.images[ibol].src=eval('bol_on'+".src");
   document.images[ipijl].src=eval('pijl_on'+".src");
  }
 }
function hiStop(ibol,ipijl) 
 {
  if (browser!="WRONG") 
  {
   document.images[ibol].src=eval('bol'+".src");
   document.images[ipijl].src=eval('pijl'+".src");
  }
 }
</SCRIPT>

