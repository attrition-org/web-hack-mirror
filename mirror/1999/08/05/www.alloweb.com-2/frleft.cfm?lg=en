<script language="javascript">

browserType = navigator.appName
browserVer = parseInt(navigator.appVersion)

 if (browserType == "Microsoft Internet Explorer" && browserVer >= 4) 
                                browser = "IE4";
								
			else browser = "other"
			
	if (browser == "IE4") {
		document.body.onmouseover=makeCool;
        document.body.onmouseout=makeNormal;
		
		}
		
			
    function makeCool() {
        src = event.toElement;
        if (src.tagName == 'A') {
            src.oldcol = 'navy';
            src.style.color = 'red'; 
        }
    }
    function makeNormal() {
        src=event.fromElement;
       if (src.tagName == 'A')  {
            src.style.color = 'navy';
        }
    }
	

	   function makeCooler() {
     
        if (src.tagName == 'A') {
            src.oldcol = 'navy';
            src.style.color = 'purple'; 
        }
    }

</script>












