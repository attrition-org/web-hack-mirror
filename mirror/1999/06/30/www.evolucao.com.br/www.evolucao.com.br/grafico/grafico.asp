<script LANGUAGE="javascript">
	window.defaultStatus="Site Evolução Informática";
    document.onmouseover=acende;
    document.onmouseout=apaga;

    function acende() {
        src = event.toElement;
        if (src.tagName == "A") {
			window.defaultStatus="Site Evolução Informática";
            src.style.color = "Orange"; 
        }
    }

    function apaga() {
        src=event.fromElement;
        if (src.tagName == "A") {
			window.defaultStatus="Site Evolução Informática";
            src.style.color = "Navy";
        }
    }
	
</script>
