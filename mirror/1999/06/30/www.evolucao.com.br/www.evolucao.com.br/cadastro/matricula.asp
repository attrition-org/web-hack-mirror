<script>
function check(){ 
	if (document.matricula.curso.value == "") {
	alert("Preencha o nome do Curso que lhe interessa!");
	document.matricula.curso.focus();
	}
	else {
	if (document.matricula.nome.value == "") {
	alert("Preencha o seu nome!");
	document.matricula.nome.focus();
	}
	else {
	if (document.matricula.telefone.value == "" && document.matricula.email.value == "" && document.matricula.fax.value == "") {
	alert("Preencha o seu e-mail, telefone ou fax!");
	document.matricula.email.focus();
	}		
	else {
	document.matricula.submit("enviar.asp");
	}}}}
</script>

