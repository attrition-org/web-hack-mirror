<!-- JUMP_MENU -->
<!-- Used on all templates to return to -->
<!-- previous MENU screen -->
<!-- for SEARCH, BRIEF, LONG and HOLDINGS templates, this typically means -->
<!-- select a new database! -->
<!-- Navigation Properties -->
<!-- JUMP_SEARCH -->
<!-- Used on SEARCH, BRIEF, LONG, HOLDINGS templates to return to -->
<!-- SEARCH screen -->
<!-- JUMP_SCAN -->
<!-- Used by BRIEF LONG and HOLDINGS templates if result set is a result -->
<!-- of a SCAN to brief operation -->
<!-- returns to SCAN record listing -->
<!-- JUMP_BRIEF -->
<!-- Used by LONG and HOLDINGS templates if result set has more than 1 hit -->
<!-- returns to BRIEF record listing -->
<!-- NEXT_PAGE -->
<!-- Used on SCAN, BRIEF, LONG and HOLDINGS menus to go the next -->
<!-- logical "page" -->
<!-- logical "page" for SCAN is next set of headings, -->
<!-- BRIEF is next 'n' brief records -->
<!-- LONG and HOLDINGS is next record in set -->
<!-- NEXT_PAGE -->
<!-- Used on SCAN, BRIEF, LONG and HOLDINGS menus to go the previous -->
<!-- logical "page" -->
<!-- logical "page" for SCAN is previous set of headings, -->
<!-- BRIEF is previous 'n' brief records -->
<!-- LONG and HOLDINGS is previous record in set -->
<!-- GO_BACK - Used by error, no_hits, and holdings templates to return -->
<!-- back to previous screen -->
