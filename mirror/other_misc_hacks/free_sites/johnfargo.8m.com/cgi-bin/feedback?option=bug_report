				str+='<input type=hidden name=js value=YES>';
				str+='<input type=hidden name=appname value="'+navigator.appName+'">';
				str+='<input type=hidden name=appvers value="'+navigator.appVersion+'">';
				str+='<INPUT TYPE="button" NAME="return" VALUE="<< Back" STYLE="width:70px;clear;" onmouseover="mmover(this)" onmouseout="mmout(this)" onfocus="mmfocus(this)" onblur="mmblur(this)" onclick="javascript:history.go(-1);"><INPUT TYPE="button" NAME="compliments_submit" VALUE="Next >>" STYLE="width:70px;clear;" onmouseover="mmover(this)" onmouseout="mmout(this)" onfocus="mmfocus(this)" onblur="mmblur(this)" onclick="validate()";>';
				document.write(str);
				document.support_form.name.focus();	// sets focus on the first control of the support form
				document.support_form.name.select();	// selects the text (if any) in the control
			//--></SCRIPT>
			<NOSCRIPT>
				<!-- this section is from the pre-kana (support cgi) form -->
					<INPUT TYPE="hidden" NAME=js VALUE=NO>
					<INPUT TYPE="hidden" NAME=appname VALUE="NON-SCRIPT BROWSER (NO appName OBJECT)">
					<INPUT TYPE="hidden" NAME=appvers VALUE="NON-SCRIPT BROWSER (NO appVersion OVJECT)">
					
        <INPUT TYPE="submit" NAME="Submit2" VALUE="Back">
					
        <INPUT TYPE="submit" NAME="compliments_submit" VALUE="Next">
				<!-- end -->
			</NOSCRIPT>
		</FONT></TD>
	</TR>
</TABLE>
<!-- end navigation bar -->


<!-- client-side validation services -->
<SCRIPT><!--
// TB 06/14/00
function validate(){
	var error_str='';
	var error_focus_set=0;

	// confirm user name is present
		var str=document.support_form.name;
		if (str.value.length<1){
			error_str+='\nPlease include your full name.';
			if (!error_focus_set) error_focus_set=str;
		}

	// validate user email address present and structured
		var str=document.support_form.email;
		if (str.value.length<1){
			error_str+='\nPlease include your e-mail address.';
			if (!error_focus_set) error_focus_set=str;
		}
		else{
			if (str.value.indexOf("@")<1){
				error_str+='\nPlease enter a valid e-mail address.';
				if (!error_focus_set) error_focus_set=str;
			}
		}

	// validate site control
		var str=document.support_form.site;
		if (str.value.length<1){
			error_str+='\nPlease include the URL to your web site.';
			if (!error_focus_set) error_focus_set=str;
		}

	// validate option is selected
		var str=document.support_form.category;
		for (var index=0; index<str.length; index++){
			var test=str[index].checked;
			if (test==true) break;
		}
		if (!test){
			error_str+='\nPlease select a category.';
			if (!error_focus_set) error_focus_set=str[0];
		}

	// validate question is present
		var str=document.support_form.question;
		if (str.value.length<1){
			error_str+='\nPlease enter a question.';
			if (!error_focus_set) error_focus_set=str[0];
		}

	// submit or display errors
		if (error_str.length>0){
			alert(error_str);
			if (error_focus_set) error_focus_set.focus();
			return false;
		}
		else{
			adjust_oneliner();
			document.support_form.submit();
		}
}


function oneliner(data){
	// purpose, if no data exists in the oneline field then use the first 50 characters of the problem field as the oneline field,
	// if however there already is data in this field then the user may have modified the field and we don't want to undo their changes.
	// the weakness... what if the user changes the problem description more than once, the oneliner field gets populated only the first time.

	if (document.support_form.subject.value.length<1){
		var str='';
		var mobj=document.support_form.question.value;
		for (var index=0; (index<mobj.length&&index<50); index++){
			str+=mobj.charAt(index);
		}
		document.support_form.subject.value=str;
	}
}
function adjust_oneliner(){
	// porpoise, prepend the category to the one-liner so tech support can more easily categorize the emails.
	var str=document.support_form.category;
	for (var index=0; index<str.length; index++){
		var test=str[index].checked;
		if (test==true) var catstr=str[index].value;
	}
	document.support_form.subject.value=catstr+': '+document.support_form.subject.value;
}
//--></SCRIPT>
<!-- end client-side validation services -->

</FORM>
</TD>
              </TR>
            </TABLE>
