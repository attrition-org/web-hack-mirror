
<!--
Version: 2.0
DB: production
SQL: select a.second_deck,a.url,a.pub_id,a.art_id,a.category_string,a.headline,a.extract,TO_CHAR((aw.lastmod_date+(3/24)),'MM/DD/YY, fmHH12fm:MI AM') as lastmod_date,TO_CHAR((aw.create_date+(3/24)),'MM/DD/YY, fmHH12fm:MI AM') as create_date,a.doc_id,TO_CHAR((aw.prod_date+(3/24)),'MM/DD/YY, fmHH12fm:MI AM') as prod_date,a.teaser,a.proj_id 
 from cd_articles a,cd_article_workflows aw 
 where a.prod_date >= '20000619000000' and a.category_string like '%3667%' and a.art_id = aw.art_id  
  order by a.prod_date desc
Cat: 3667
-->

