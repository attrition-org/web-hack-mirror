<!--
Version: 2.0
DB: production
SQL: select a.second_deck,a.url,a.pub_id,a.art_id,a.category_string,a.headline,a.extract,a.doc_id,a.teaser,a.proj_id,aw.lastmod_date,aw.create_date,aw.prod_date 
 from cd_articles a,cd_article_workflows aw 
 where a.prod_date >= '20000712000000' and (a.proj_id in ('TWB','INV','NIN','REU')) and (a.category_string like '%2051%' or a.category_string like '%2049%' or a.category_string like '%2050%') and a.prev_id is null and a.art_id = aw.art_id  
  order by a.prod_date desc
Cat: 2051,2049,2050
