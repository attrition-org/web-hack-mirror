SQL: select a.second_deck,a.url,a.pub_id,a.art_id,a.images,p.pub_name,a.stock_symbol,a.headline,a.extract,TO_CHAR((aw.lastmod_date+(3/24)),'MM/DD/YY, fmHH12fm:MI AM') as lastmod_date,TO_CHAR((aw.create_date+(3/24)),'MM/DD/YY, fmHH12fm:MI AM') as create_date,a.doc_id,a.byline,TO_CHAR((aw.prod_date+(3/24)),'MM/DD/YY, fmHH12fm:MI AM') as prod_date,a.teaser,a.proj_id,p.pub_url,a.prev_id,a.search_query,a.next_id,a.body 
 from cd_pubs p,cd_articles a,cd_article_workflows aw 
 where a.doc_id = 'TWB19990219S0004' and a.art_id = aw.art_id and a.pub_id = p.pub_id (+)  
  order by a.prod_date desc
